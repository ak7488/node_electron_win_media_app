{
    "name": "video-app",
    "productName": "Video",
    "version": "1.0.0",
    "description": "A video app for desktop",
    "main": "main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "electron main.js",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/video.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . video-app --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/1video.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Video\""
    },
    "author": "Anubhav_Anand",
    "license": "ISC",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "electron": "^12.0.6",
        "electron-packager": "^15.2.0",
        "electron-winstaller": "^5.0.0"
    }
}
